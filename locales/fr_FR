DATE_YESTERDAY
	Hier à {hour}:{min}
DATE_FULL
	{day} {date} {month} à {hour}:{min}
DATE_FULL_WITH_YEAR
	{day} {date} {month} {year} à {hour}:{min}
DATE_MONTH
	{day} {date} {month}
DATE_MONTH_WITH_YEAR
	{day} {date} {month} {year}
DATE_EVENT_TYPE1
	{day} {date} {month} {year}, de {hour1}:{min1} à {hour2}:{min2}
DATE_EVENT_TYPE2
	Du {day1} {date1} {month1} {year1} à {hour1}:{min1} au {day2} {date2} {month2} {year2} à {hour2}:{min2}
